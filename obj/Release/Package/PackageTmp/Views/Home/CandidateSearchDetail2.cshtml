                                                                                        <select class="form-control" id="wh_toyear" name="wh_toyear" @(e.CurrentlyWorkingHere == true ? "disabled" : "")>
                                                                                            <option value="">Select</option>
                                                                                            @for (int x = DateTime.Now.Year; x >= 1970; x--)
                                                                                            {
                                                                                                <option @(e.ToYear == x ? "selected" : "") value="@x">@x</option>
                                                                                            }


                                                                                        </select>
                                                                                    </div>
