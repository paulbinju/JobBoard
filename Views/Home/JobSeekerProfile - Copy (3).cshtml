                        <div class="moneytran-img  dashboardwhiteboxshadow  fade-1" style="padding-bottom:10px"> <a href="#"><img src="images/money-transfer-img.jpg" alt="" /></a> </div>
                        <div class="ingrambox dashboardwhiteboxshadow  fade-1">
                            <!-- InstaWidget -->
                            <a href="https://instawidget.net/v/user/jobs4bahrainis" id="link-b660c32d2a4cc8d4e0535b6a7f91ed8d34c89d205c8a48c58a15aad3f0c1585a">@@jobs4bahrainis</a>
                            <script src="https://instawidget.net/js/instawidget.js?u=b660c32d2a4cc8d4e0535b6a7f91ed8d34c89d205c8a48c58a15aad3f0c1585a&width=250px"></script>
                        </div>
                    </div>
                </div>
                <div class="col-sm-9">
                    <div class="homesearchsection  ">
                        <h1 class="mainpagetitle Padd15"><span>JOBSEEKER </span>PROFILE</h1>
                        <div class="searchformsec">
                            <div class="row" style="font-size:15px;padding:0 5px">


                                <div class="row">
                                    <div class="col-md-3 pull-right"><a target="_blank" href="@Url.Content("~/CVViewer/"+ViewBag.Ca_GUID)" class="btnsubmit pull-right">VIEW CV</a> </div>
                                    <div class="col-md-8" style="font-size:22px;"><b>@ViewBag.Name</b></div>
                                </div>
                                <br />

                                <div class="row">

                                    <div class="col-md-6" style="font-size:14px"><b>Phone:</b> @ViewBag.Phone</div>
                                    <div class="col-md-6"><b style="font-size:14px">Email:</b> @ViewBag.Email</div>
                                    <div class="col-md-6"><b style="font-size:14px">Date of Birth:</b> @ViewBag.DOB</div>
                                    <div class="col-md-6"><b style="font-size:14px">Gender:</b> @(ViewBag.ca_Gender == "M" ? "Male" : "Female")</div>
                                    <div class="col-md-6"><b style="font-size:14px">Marital Status:</b> @ViewBag.MaritalStatus</div>
                                    <div class="col-md-6"><b style="font-size:14px">Nationality:</b> @ViewBag.Nationality</div>
                                    <div class="col-md-6"><b style="font-size:14px">Current Location:</b> @ViewBag.CurrentLocation</div>
                                    <div class="col-md-6"><b style="font-size:14px">Education Level:</b> @ViewBag.HighestEducation</div>
                                    <div class="col-md-6"><b style="font-size:14px">Name of University:</b> @ViewBag.University2</div>
                                    <div class="col-md-6"><b style="font-size:14px">Current Job Function:</b> @ViewBag.FunctionTitle</div>
                                    <div class="col-md-6"><b style="font-size:14px">Current Job Title:</b> @ViewBag.CurrentJobTitle</div>
                                    <div class="col-md-6"><b style="font-size:14px">Total Experience:</b> @ViewBag.TotalExperience</div>
                                    <div class="col-md-12"><b style="font-size:14px">Profile Summary:</b> @Html.Raw(ViewBag.ca_Profile)</div>
                                </div>

                            </div>
                        </div>
                    </div>


                    <div class="homesearchsection">
                        <h1 class="mainpagetitle Padd15"><span>WORK </span>HISTORY</h1>
                        <div class="searchformsec">
                            <table class="table table-bordered table-responsive" style="font-size:15px;">
                                <tr>
                                    <th>Job</th>
                                    <th>Company</th>
                                    <th style="width:50px;">Modify</th>


                                </tr>
                        
                                @foreach (var e in ViewBag.Experience2)
                                {
                                   
                                    <tr>
                                        <td>
                                            <b>@e.JobTitle</b>
                                            <br />
                                            <b>Job Level:</b> @e.JobLevel<br />
                                            <b>Industry:</b> @e.Industry<br />
                                            <b>Function:</b> @e.JobFunction<br />
                                            <b>Reporting to:</b> @e.ReportingTo
                                        </td>
                                        <td>
                                            <b>@e.Company</b><br />
                                            @if (e.CurrentlyWorkingHere == false)
                                            {
                                                <span>
                                                    <b>Period:</b> @cmn.GetMonthName(e.FromMonth) @e.FromYear to @cmn.GetMonthName(e.ToMonth) @e.ToYear <br />
                                                </span>
                                            }
                                            else
                                            {
                                                <span>
                                                    <b>Currently working here:</b> Since @cmn.GetMonthName(e.FromMonth) @e.FromYear <br />
                                                </span>
                                            }
                                            <b>Location:</b> @e.JobLocation <br />
                                            <b>Salary:</b> BHD @e.Salary
                                        </td>
                                        <td rowspan="2"><i class="fa fa-edit" style="color:red;font-size:20px;cursor:pointer" data-toggle="modal" data-target="#whmodal-@e.CaWorkHistory_ID"></i></td>
                                    </tr>
                                    <tr>
                                        <td colspan="2"><b>Activities:</b> @Html.Raw(e.Description)</td>



                                    </tr>


                                    <div id="whmodal-@e.CaWorkHistory_ID" class="modal fade" role="dialog">
                                        <div class="modal-lg center-block">
                                            <div class="registerformbox">
                                                <div class="modal-content">
                                                    <div class="modal-header">
                                                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                                                        <h1 class="mainpagetitle"><span>WORK </span>HISTORY</h1>
                                                    </div>
                                                    <div class="modal-body">
                                                        <div class="row">
                                                            <div class="col-sm-12">

                                                                <div class="registerformbox">
                                                                    <div class="row" style="min-height:500px;">
                                                                        <form method="post" action="@Url.Content("~/JobSeekerWorkHistoryUpdate")" name="whformz">
                                                                            <div id="whform">
                                                                                <div class="col-sm-6">
                                                                                    <div class="form-group">
                                                                                        <label>Job Title <span>*</span></label>

                                                                                        <select style="width:100%" class="form-control searchableselect"  name="wh_jobtitle" required oninvalid="this.setCustomValidity('Job Title is mandatory')" oninput="setCustomValidity('')">
                                                                                            <option value="">Select</option>
                                                                                            @foreach (var jfn in ViewBag.JobTitle)
                                                                                            {
                                                                                                <option @(e.JobTitleID == jfn.lm_Id ? "selected" : "") value="@jfn.lm_Id">@jfn.lm_Value</option>
                                                                                            }

                                                                                        </select>


                                                                                    </div>

                                                                                </div>
                                                                                <div class="col-sm-6">
                                                                                    <div class="form-group   ui-front">
                                                                                        <label>Company / Organization <span>*</span></label>
                                                                                        <input value="@e.Company" class="form-control" id="wh_company" name="wh_company" placeholder="Select" type="text" required oninvalid="this.setCustomValidity('Company is mandatory')" oninput="setCustomValidity('')">
                                                                                    </div>

                                                                                </div>
                                                                                <div class="col-sm-6">
                                                                                    <div class="form-group">
                                                                                        <label>Level (within the organization) <span>*</span></label>


                                                                                        <select style="width:100%" class="form-control searchableselect"   name="wh_level" required oninvalid="this.setCustomValidity('Job Level is mandatory')" oninput="setCustomValidity('')">
                                                                                            <option value="">Select</option>
                                                                                            @foreach (var jfn in ViewBag.JobLevel)
                                                                                            {
                                                                                                <option @(e.JobLevelID == jfn.lm_Id ? "selected" : "") value="@jfn.lm_Id">@jfn.lm_Value</option>
                                                                                            }

                                                                                        </select>

                                                                                    </div>
                                                                                </div>
                                                                                <div class="col-sm-6">
                                                                                    <div class="form-group">
                                                                                        <label for="gender">Reporting to <span>*</span></label>


                                                                                        <select style="width:100%" class="form-control searchableselect"   name="wh_reportingto" required oninvalid="this.setCustomValidity('Job Level is mandatory')" oninput="setCustomValidity('')">
                                                                                            <option value="">Select</option>
                                                                                            @foreach (var jfn in ViewBag.Reportingto)
                                                                                            {
                                                                                                <option @(e.ReportingToID == jfn.lm_Id ? "selected" : "") value="@jfn.lm_Id">@jfn.lm_Value</option>
                                                                                            }

                                                                                        </select>
                                                                                    </div>
                                                                                </div>


                                                                                <div class="col-sm-6">
                                                                                    <div class="form-group">
                                                                                        <label>Industry <span>*</span></label>

                                                                                        <select style="width:100%" class="form-control searchableselect"  name="wh_industry" required oninvalid="this.setCustomValidity('Industry is mandatory')" oninput="setCustomValidity('')">
                                                                                            <option value="">Select</option>
                                                                                            @foreach (var ind in ViewBag.Sectors)
                                                                                            {
                                                                                                <option @(e.IndustryID == ind.lm_Id ? "selected" : "") value="@ind.lm_Id">@ind.lm_Value</option>
                                                                                            }
                                                                                        </select>
                                                                                    </div>
                                                                                </div>
                                                                                <div class="col-sm-6">
                                                                                    <div class="form-group">
                                                                                        <label for="currentposition">Function <span>*</span></label>
                                                                                        <select style="width:100%" class="form-control searchableselect"  name="wh_function" required oninvalid="this.setCustomValidity('Function is mandatory')" oninput="setCustomValidity('')">
                                                                                            <option value="">Select</option>
                                                                                            @foreach (var jfn in ViewBag.FunctionalTitles)
                                                                                            {
                                                                                                <option @(e.FunctionID == jfn.lm_Id ? "selected" : "") value="@jfn.lm_Id">@jfn.lm_Value</option>
                                                                                            }

                                                                                        </select>

                                                                                    </div>
                                                                                </div>

                                                                                <div class="col-md-3">
                                                                                    <div class="form-group">
                                                                                        <label>From <span>*</span></label>
                                                                                        <select class="form-control"  name="wh_frommonth" required>
                                                                                            <option value="">Month</option>
                                                                                            <option @(e.FromMonth == 1 ? "selected" : "") value="1">January</option>
                                                                                            <option @(e.FromMonth == 2 ? "selected" : "") value="2">February</option>
                                                                                            <option @(e.FromMonth == 3 ? "selected" : "") value="3">March</option>
                                                                                            <option @(e.FromMonth == 4 ? "selected" : "") value="4">April</option>
                                                                                            <option @(e.FromMonth == 5 ? "selected" : "") value="5">May</option>
                                                                                            <option @(e.FromMonth == 6 ? "selected" : "") value="6">June</option>
                                                                                            <option @(e.FromMonth == 7 ? "selected" : "") value="7">July</option>
                                                                                            <option @(e.FromMonth == 8 ? "selected" : "") value="8">August</option>
                                                                                            <option @(e.FromMonth == 9 ? "selected" : "") value="9">September</option>
                                                                                            <option @(e.FromMonth == 10 ? "selected" : "") value="10">October</option>
                                                                                            <option @(e.FromMonth == 11 ? "selected" : "") value="11">November</option>
                                                                                            <option @(e.FromMonth == 12 ? "selected" : "") value="12">December</option>
                                                                                        </select>
                                                                                    </div>
                                                                                </div>
                                                                                <div class="col-md-3">
                                                                                    <div class="form-group">
                                                                                        <label>&nbsp;</label>
                                                                                        <select class="form-control"  name="wh_fromyear" id="wh_fromyear-@e.CaWorkHistory_ID" required onchange="settoyear@(e.CaWorkHistory_ID)()">
                                                                                            <option value="">Select</option>
                                                                                            @for (int x = DateTime.Now.Year; x >= 1970; x--)
                                                                                            {
                                                                                                <option @(e.FromYear == x ? "selected" : "") value="@x">@x</option>
                                                                                            }


                                                                                        </select>
                                                                                    </div>
                                                                                </div>
                                                                                <div class="col-md-3">
                                                                                    <div class="form-group">
                                                                                        <label>To <span>*</span></label>
                                                                                        <select class="form-control"  id="wh_tomonth-@e.CaWorkHistory_ID" required  name="wh_tomonth" @(e.CurrentlyWorkingHere == true ? "disabled" : "")>
                                                                                            <option value="">Month</option>
                                                                                            <option @(e.ToMonth == 1 ? "selected" : "") value="1">January</option>
                                                                                            <option @(e.ToMonth == 2 ? "selected" : "") value="2">February</option>
                                                                                            <option @(e.ToMonth == 3 ? "selected" : "") value="3">March</option>
                                                                                            <option @(e.ToMonth == 4 ? "selected" : "") value="4">April</option>
                                                                                            <option @(e.ToMonth == 5 ? "selected" : "") value="5">May</option>
                                                                                            <option @(e.ToMonth == 6 ? "selected" : "") value="6">June</option>
                                                                                            <option @(e.ToMonth == 7 ? "selected" : "") value="7">July</option>
                                                                                            <option @(e.ToMonth == 8 ? "selected" : "") value="8">August</option>
                                                                                            <option @(e.ToMonth == 9 ? "selected" : "") value="9">September</option>
                                                                                            <option @(e.ToMonth == 10 ? "selected" : "") value="10">October</option>
                                                                                            <option @(e.ToMonth == 11 ? "selected" : "") value="11">November</option>
                                                                                            <option @(e.ToMonth == 12 ? "selected" : "") value="12">December</option>
                                                                                        </select>
                                                                                    </div>
                                                                                </div>
                                                                                <div class="col-md-3">
                                                                                    <div class="form-group">
                                                                                        <label>&nbsp;</label>
                                                                                        <select class="form-control"  id="wh_toyear-@e.CaWorkHistory_ID" required  name="wh_toyear" @(e.CurrentlyWorkingHere == true ? "disabled" : "")>
                                                                                            <option value="">Select</option>
                                                                                            @for (int x = DateTime.Now.Year; x >= 1970; x--)
                                                                                            {
                                                                                                <option @(e.ToYear == x ? "selected" : "") value="@x">@x</option>
                                                                                            }


                                                                                        </select>
                                                                                    </div>
                                                                                </div>
                                                                                <div class="col-sm-12">
                                                                                    <div class="form-group">


                                                                                        <div class="form-check">
                                                                                            <input id="wh_currentlywork-@e.CaWorkHistory_ID" name="wh_currentlywork" class="form-check-input" type="checkbox" value="True" style="float:left" @(e.CurrentlyWorkingHere == true ? "checked" : "")>
                                                                                            <div>Currently Studying here</div>

                                                                                            <input type="hidden" name="wh_tomonth" id="wh_tomonth1-@e.CaWorkHistory_ID" value="0" />
                                                                                            <input type="hidden" name="wh_toyear" id="wh_toyear1-@e.CaWorkHistory_ID" value="0" />

                                                                                            <script>
                                                                                        $("#wh_currentlywork-@e.CaWorkHistory_ID").click(function () {
                                                                                            if ($("#wh_currentlywork-@e.CaWorkHistory_ID").is(":checked")) {
                                                                                                $("#wh_tomonth-@e.CaWorkHistory_ID,#wh_toyear-@e.CaWorkHistory_ID").prop("disabled", true);
                                                                                                $("#wh_tomonth1-@e.CaWorkHistory_ID,#wh_toyear1-@e.CaWorkHistory_ID").prop("disabled", false);
                                                                                                $("#wh_tomonth-@e.CaWorkHistory_ID,#wh_toyear-@e.CaWorkHistory_ID").val("").change();
                                                                                            }
                                                                                            else {
                                                                                                $("#wh_tomonth-@e.CaWorkHistory_ID,#wh_toyear-@e.CaWorkHistory_ID").prop("disabled", false);
                                                                                                $("#wh_tomonth1-@e.CaWorkHistory_ID-@e.CaWorkHistory_ID,#wh_toyear1").prop("disabled", true);
                                                                                            }
                                                                                        });

                                                                                                $(document).ready(function () {

                                                                                                    if ($("#wh_currentlywork-@e.CaWorkHistory_ID").is(":checked")) {
                                                                                                        $("#wh_tomonth-@e.CaWorkHistory_ID,#wh_toyear-@e.CaWorkHistory_ID").prop("disabled", true);
                                                                                                        $("#wh_tomonth1-@e.CaWorkHistory_ID,#wh_toyear1-@e.CaWorkHistory_ID").prop("disabled", false);

                                                                                                    }
                                                                                                    else {
                                                                                                        $("#wh_tomonth-@e.CaWorkHistory_ID,#wh_toyear-@e.CaWorkHistory_ID").prop("disabled", false);
                                                                                                        $("#wh_tomonth1-@e.CaWorkHistory_ID,#wh_toyear1-@e.CaWorkHistory_ID").prop("disabled", true);
                                                                                                    }
                                                                                                });

                                                                                                function settoyear@(e.CaWorkHistory_ID)() {

                                                                                                    $('#wh_toyear-@e.CaWorkHistory_ID').empty();
                                                                                                    for (i = @DateTime.Now.Year;i >= $('#wh_fromyear-@e.CaWorkHistory_ID').val(); i--) {
                                                                                                        $('#wh_toyear-@e.CaWorkHistory_ID').append($('<option>',
                                                                                                            {
                                                                                                                value: i,
                                                                                                                text: i
                                                                                                            }));
                                                                                                    }
                                                                                                }


                                                                                            </script>




                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                                <div class="col-sm-6">
                                                                                    <div class="form-group">

                                                                                        <label>Job Location <span>*</span></label>
                                                                                        <select style="width:100%" class="form-control searchableselect"    name="wh_location" required oninvalid="this.setCustomValidity('Gender is mandatory')" oninput="setCustomValidity('')">
                                                                                            <option value="">Select</option>
                                                                                            @foreach (var con in ViewBag.Country)
                                                                                            {
                                                                                                <option @(e.JobLocationID == con.lm_Id ? "selected" : "") value="@con.lm_Id">@con.lm_Value</option>
                                                                                            }
                                                                                        </select>
                                                                                    </div>

                                                                                </div>

                                                                                <div class="col-sm-6">
                                                                                    <div class="form-group">
                                                                                        <label for="currentposition">Salary <span>*</span></label>

                                                                                        <select style="width:100%" class="form-control searchableselect"    name="wh_salary" required oninvalid="this.setCustomValidity('Salary is mandatory')" oninput="setCustomValidity('')">
                                                                                            <option value="">Select</option>
                                                                                            @foreach (var con in ViewBag.Salary)
                                                                                            {
                                                                                                <option @(e.SalaryID == con.lm_Id ? "selected" : "") value="@con.lm_Id">@con.lm_Value</option>
                                                                                            }
                                                                                        </select>
                                                                                    </div>
                                                                                </div>

                                                                                <div class="col-sm-12">
                                                                                    <div class="form-group">
                                                                                        <label for="summary">Description</label>
                                                                                        <textarea class="form-control" name="description"   placeholder="Job description, skills used at work etc">@e.Description</textarea>
                                                                                    </div>
                                                                                </div>




                                                                                <div class="col-sm-12">
                                                                                    <div class="form-group formbtn ">
                                                                                        @if (ViewBag.Experience2.Count >= 2)
                                                                                        {
                                                                                            <div class="text-left   pull-left">
                                                                                                <input type="button" value="Delete" class="graybtn" onclick="delWH(@e.CaWorkHistory_ID)">
                                                                                            </div>
