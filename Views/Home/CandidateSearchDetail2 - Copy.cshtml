                CertificationID: $('#cert_title').val(), AuthorityID: $('#cert_auth').val(), FromMonth: $('#cert_frommonth').val(), FromYear: $('#cert_fromyear').val(), ToMonth: $('#cert_tomonth').val(), ToYear: $('#cert_toyear').val(), DoNotExpire: cert_noexpiry
