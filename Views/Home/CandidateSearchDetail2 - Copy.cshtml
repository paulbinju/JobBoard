<div id="candidateModal" class="modal fade" role="dialog">
    <div class="modal-lg center-block">
        <div class="registerformbox">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h1 class="mainpagetitle">JOBSEEKER <span>PROFILE</span></h1>
                </div>
                <div class="modal-body">

                    <div class="row">

                        <div class="registerformbox" id="registerformbox">
                            <form id="mainform" method="post" enctype="multipart/form-data">
                                <div class="row" style="min-height:500px;">


                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label>Date of Birth <span>*</span></label>


                                            <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>


                                            <input value="@ViewBag.DOB" class="form-control" id="ca_DateOfBirth" name="ca_DateOfBirth" placeholder="Date / Month / Year" type="text" onkeydown="return false;" required>

                                            <script>
                                                $(document).ready(function () {
                                                    var options = $.extend({},
                                                        $.datepicker.regional["ja"], {
                                                            dateFormat: "yy/mm/dd",
                                                            changeMonth: true,
                                                            changeYear: true,
                                                            yearRange: "1975:2015"
                                                        }
                                                    );
                                                    $("#ca_DateOfBirth").datepicker(options);
                                                });

                                            </script>

                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label for="gender">Gender <span>*</span></label>
                                            <select class="form-control" id="ca_Gender" name="ca_Gender" required oninvalid="this.setCustomValidity('Gender is mandatory')" oninput="setCustomValidity('')">
                                                <option value="">Select</option>
                                                <option @(ViewBag.Gender == "M" ? "selected" : "") value="M">Male</option>
                                                <option @(ViewBag.Gender == "F" ? "selected" : "") value="F">Female</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label>Marital Status <span>*</span></label>
                                            <select class="form-control" id="ca_MaritalStatus" name="ca_MaritalStatus" required oninvalid="this.setCustomValidity('Marital Status is mandatory')" oninput="setCustomValidity('')">
                                                <option value="">Select</option>
                                                <option @(ViewBag.ca_MaritalStatus == "Single" ? "selected" : "") value="Single">Single</option>
                                                <option @(ViewBag.ca_MaritalStatus == "Married" ? "selected" : "") value="Married">Married</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label>Current Location <span>*</span></label>
                                            <select class="form-control" id="ca_lm_CountryId" name="ca_lm_CountryId" required oninvalid="this.setCustomValidity('Current Location is mandatory')" oninput="setCustomValidity('')">
                                                <option value="">Select</option>
                                                @foreach (var con in ViewBag.Country)
                                                {

                                                    <option @(ViewBag.Ca_CurrentCountryID == con.lm_Id ? "selected" : "") value="@con.lm_Id">@con.lm_Value</option>
                                                }
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label for="highesteducation">Highest Level of Education <span>*</span></label>
                                            <select class="form-control" id="ca_lm_EducationLevel" name="ca_lm_EducationLevel" required oninvalid="this.setCustomValidity('Highest level of education is mandatory')" oninput="setCustomValidity('')">
                                                <option value="">Select</option>
                                                @foreach (var edu in ViewBag.EducationLevel)
                                                {
                                                    <option @(ViewBag.ca_lm_EducationLevel == edu.lm_Id ? "selected" : "") value="@edu.lm_Id">@edu.lm_Value</option>
                                                }
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label>Name of High School / University </label>
                                            <select class="form-control" id="proUniversity" name="proUniversity" required oninvalid="this.setCustomValidity('Name of High School / University')" oninput="setCustomValidity('')">
                                                <option value="">Select</option>
                                                @foreach (var uni in ViewBag.University)
                                                {
                                                    <option @(ViewBag.ca_UniversityID == uni.lm_Id ? "selected" : "") value="@uni.lm_Id">@uni.lm_Value</option>
                                                }
                                            </select>
                                        </div>
                                    </div>


                                    <div id="freshcheck">

                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label for="currentposition">Current Job Function </label>
                                                <select class="form-control" id="ca_FunctionTitleID" name="ca_FunctionTitleID" required oninvalid="this.setCustomValidity('Current Job Function is mandatory')" oninput="setCustomValidity('')">
                                                    <option value="">Select</option>
                                                    @foreach (var jfn in ViewBag.FunctionalTitles)
                                                    {
                                                        <option @(ViewBag.ca_FunctionTitleID == jfn.lm_Id ? "selected" : "") value="@jfn.lm_Id">@jfn.lm_Value</option>
                                                    }

                                                </select>

                                            </div>
                                        </div>

                                        <div class="col-sm-6">
