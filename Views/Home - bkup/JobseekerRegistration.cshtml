    function idelcareermove() {
        if ($('#icm_jobfunction').val() != "") {

            var sal = $('#icm_salary').val().split('-');
            var salfrom = sal[0];
            var salto = sal[1];

            $.ajax({
                method: "POST",
                url: "JobseekerCareerMove",
                data: { Ca_Id: 1050, SectorID: $('#icm_industry').val(), JobTitle: $('#icm_jobfunction').val(), SalaryFrom: salfrom, SalaryTo: salto, ExperienceYears: $('#icm_experience').val() },
                success: function (data) {
                    alert('Successfully added!');
                },
                error: function () {
                    alert("Couldn't Save, check the input values!");
                }
            });

        }
        else {
            alert('Job function is mandatory');
        }
    }


    function addskill() {
        if ($('#skill_add').val() != "") {


            $.ajax({
                method: "POST",
                url: "JobseekerSkills",
                data: { Ca_Id: 1050, Skills: $('#skill_add').val(), SkillLevel: $('#skill_level').val()},
                success: function (data) {
                    alert('Successfully added!');
                },
                error: function () {
                    alert("Couldn't Save, check the input values!");
                }
            });

        }
        else {
            alert('Skill is mandatory');
        }
    }

    function addEdu() {
        if ($('#eduSchool').val() != "") {
            var educurent = "";
            if ($("#educurrentlystudy").not(':checked')) {
                educurent = "false";
            }
            else {
                educurent = "true";

            }

            $.ajax({
                method: "POST",
                url: "JobSeekerEducation",
                data: { Ca_ID: 1500, DegreeLevel: $('#eduDegree').val(), MajorSubject: $('#eduField').val(), Institute: $('#eduSchool').val(), Grade: $('#eduGrade').val(), FromDate: $('#edufrommonth').val() + ' ' + $('#edufromyear').val(), ToDate: $('#edutomonth').val() + ' ' + $('#edutoyear').val(), Description: $('#eduDescription').val(), Activities: $('#eduActivities').val(), CurrentlyStudyinghere: educurent },
                success: function (data) {
                    alert('Successfully added!');

                },
                error: function () {
                    alert("Couldn't Save, check the input values!");
                }
            });

        }
        else {
            alert('All fields are mandatory');
            $("#eduform2").hide();
            $("#eduform").show();
        }
    }

    function addWorkHistory() {
        if ($('#wh_jobtitle').val() != "") {


            $.ajax({
                method: "POST",
