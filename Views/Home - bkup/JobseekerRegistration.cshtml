                data: { Ca_ID: 1150, JobTitle: $('#wh_jobtitle').val(), Company: $('#wh_company').val(), TotalExp: 0, City: $('#wh_location').val(), joblevel: $('#wh_level').val(), reportingto: $('#wh_reportingto').val(), industry: $('#wh_industry').val(), FromDate: $('#wh_frommonth').val() + ' ' + $('#wh_fromyear').val(), ToDate: $('#wh_tomonth').val() + ' ' + $('#wh_toyear').val(), salary: $('#wh_salary').val(), description: $('#wh_description').val()  },
