                                                   <option value="Single">Single</option>
                                                   <option value="Married">Married</option>
                                               </select>
                                           </div>
                                       </div>
                                       <div class="col-sm-6">
                                           <div class="form-group">
                                               <label for="currentposition">Job Function </label>
                                               <select class="form-control" id="currentposition" required>
                                                   <option value="">Select</option>
                                                   @foreach (var jfn in ViewBag.FunctionalTitles)
                                                   {
                                                       <option value="@jfn.lm_Id">@jfn.lm_Value</option>
                                                   }

                                               </select>

                                           </div>
                                       </div>

                              
                                       <div class="col-sm-6">
                                           <div class="form-group">
                                               <label for="highesteducation">Highest Level of Education <span>*</span></label>
                                               <select class="form-control" id="highesteducation" required>
                                                   <option value="">Select</option>
                                                   @foreach (var edu in ViewBag.EducationLevel)
                                                   {
                                                       <option value="@edu.lm_Id">@edu.lm_Value</option>
                                                   }


                                               </select>
                                           </div>
                                       </div>
                                       <div class="col-sm-6">
                                           <div class="form-group">
                                               <label>Expected Monthly Salary (BHD) <span>*</span></label>

                                               <input class="form-control" id="expectedsalary" type="number" step=".5" required min="100" max="5000"  title="BHD between 100 - 5000">
